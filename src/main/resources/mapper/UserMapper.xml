<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.fs.mapper.UserMapper">


    <select id="selectUserByUserName" resultType="org.example.fs.domain.entity.User">
        select *
        from user
        where username = #{username}
    </select>
</mapper>
